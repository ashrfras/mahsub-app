ئورد مكون، عنصرمخصص؛
ئورد فونت من مكون.فونت؛
ئورد مكون.ئيقو؛
ئورد مكون.حقلئدخ؛
ئورد مكون.زرئدخ؛
ئورد مكون.بطاقة؛
ئورد مكون.شبكي؛
ئورد مكون.طافية.طافية؛
ئعلن تطبيق ()؛
ئعلن تطبيق.حممل ()؛
ئعلن تطبيق.صيير ()؛
ئعلن مدخل ()؛
ئعلن التطبيق ك تطبيق؛
ئعلن تطبيق ():
	يمدد مكون()؛
	يملك مركب {
		نصية صفحة
	} حالة = {
		صفحة: 'رئيسية'
	}؛
تم

ئعلن تطبيق.حممل ():
	التطبيق = هدا؛
	هدا.خدحالة({
		صفحة: 'تئريخ'
	})؛
تم

ئعلن تطبيق.صيير ():
	ئرجع (
		<فونت ئسم="مركزي" عنونت="ملفا/MarkaziTextRegular.ttf"></فونت>
		<فونت ئسم="مركزي" عنونت="ملفا/MarkaziTextBold.ttf" وزن="سميك"></فونت>
		<تدويل عنونت="/لغات" مفترض="ar"></تدويل>
		<شرط>
			<عند تحقق="هدا.حالة.صفحة == 'رئيسية'">
				<رئيسية></رئيسية>
			</عند>
			<عند تحقق="هدا.حالة.صفحة == 'دخول'">
				<دخول></دخول>
			</عند>
			<عند تحقق="هدا.حالة.صفحة == 'مبيع'">
				<مبيع></مبيع>
			</عند>
			<عند تحقق="هدا.حالة.صفحة == 'ولوج'">
				<ولوج></ولوج>
			</عند>
			<عند تحقق="هدا.حالة.صفحة == 'مخزون'">
				<مخزون></مخزون>
			</عند>
			<عند تحقق="هدا.حالة.صفحة == 'مستخدمين'">
				<مستخدمين></مستخدمين>
			</عند>
			<عند تحقق="هدا.حالة.صفحة == 'تئريخ'">
				<تئريخ></تئريخ>
			</عند>
			<عند تحقق="هدا.حالة.صفحة == 'ئعداد'">
				<ئعداد></ئعداد>
			</عند>
			<عند غيره>
				<حيز صنف="نصصج">صفحة غير موجودة</حيز>
			</عند>
		</شرط>
	)؛
تم

ئعلن مدخل ():
	عنصرمخصص.سججل('تطبيق'، تطبيق)؛
تم
