ئورد مكون، عنصرمخصص، عنصرئشتمل؛
ئورد التطبيق من تطبيق؛
ئورد مجال من مصفوفة؛
ئورد مشهد.رئسئدارة؛
ئورد الكل ك مجالمستخدم من صنفمجال.مستخدم؛
ئورد مختطمستخدم من صنفمجال.مستخدم؛
ئعلن مستخدمين ()؛
ئعلن مستخدمين._صيير ()؛
ئعلن مستخدمين.حممل () ك وعد؛
ئعلن مستخدمين.عندحدتنقر (عنصرئشتمل عنص)؛
ئعلن مستخدمين.صيير ()؛
ئعلن مدخل ()؛
ئعلن مستخدمين ():
	يمدد مكون()؛
	يملك مختطمستخدم[] مستخدمين = []؛
تم

ئعلن مستخدمين._صيير ():
	ئرجع (
		<طافية معرف="طفتعديلمستخدم" حجم="كبير" عنصر-بطاقة="مقوسكب:هام حش-0:هام هش-0:هام موضع-مطلق فوق-2"></طافية>
		<ئيقومنتج></ئيقومنتج>
		<قسم صنف="مخطط-طولي خلف-رمادي-200" سمة="height:100dvh">
			<رئسئدارة صفحة="مستخدم"></رئسئدارة>

			<حيز صنف="خطسميك هش-2 هشتح-0 هشفو-4">المسؤولون</حيز>
			<قسم صنف="مخطط-طولي فجوة-2 هش-3 هشفو-0 فائض-تلقا فائض-لمسي">
				<تكرار لكل="ع" في="مجال\(0, 3\)">
					<قسم صنف="مقوسكب خلف-ئبيض مخطط-عرضي حش-2 ئرصف-بين ئصفف-وسط">
						<قسم صنف="مخطط-طولي فجوة-1 نصصغ">
							<حيز>الاسم: محمد العامري</حيز>
							<حيز>رمز الولوج: 34يب45</حيز>
						</قسم>
						<ئيقو ئسم="تلاتنقط-ئفقي" صنف="نصكج مئشر-سبابة"></ئيقو>
					</قسم>
				</تكرار>
			</قسم>

			<حيز صنف="خطسميك هش-2 هشتح-0 هشفو-4">البائعون</حيز>
			<قسم صنف="مخطط-طولي فجوة-2 هش-3 هشفو-0 فائض-تلقا فائض-لمسي">
				<تكرار لكل="ع" في="مجال\(0, 5\)">
					<قسم صنف="مقوسكب خلف-ئبيض مخطط-عرضي حش-2 ئرصف-بين ئصفف-وسط">
						<قسم صنف="مخطط-طولي فجوة-1 نصصغ">
							<حيز>الاسم: محمد العامري</حيز>
							<حيز>رمز الولوج: 34يب45</حيز>
						</قسم>
						<ئيقو ئسم="تلاتنقط-ئفقي" صنف="نصكج مئشر-سبابة"></ئيقو>
					</قسم>
				</تكرار>
			</قسم>
		</قسم>
	)؛
تم

ئعلن مستخدمين.حممل () ك وعد:
	هدا.مستخدمين = ريتما مجالمستخدم.ردجميع()؛
	قل هدا.مستخدمين؛
	هدا.حددت()؛
تم
ئعلن مستخدمين.عندحدتنقر (عنصرئشتمل عنص):
	ئدا عنص.معرف == 'ئيقومخزون':
		التطبيق.خدحالة({
			صفحة: 'مخزون'
		})؛
	تم
	ئدا عنص.معرف == 'ئيقومستخدم':
		التطبيق.خدحالة({
			صفحة: 'مستخدمين'
		})؛
	تم
تم

ئعلن مستخدمين.صيير ():
	مختطمستخدم مست؛
	ئرجع (
		<طافية معرف="طفتعديلمنتج" حجم="كبير" عنصر-بطاقة="مقوسكب:هام حش-0:هام هش-0:هام موضع-مطلق فوق-2"></طافية>
		<ئيقومنتج></ئيقومنتج>
		<قسم صنف="مخطط-طولي خلف-رمادي-200" سمة="height:100dvh">
			<رئسئدارة صفحة="مخزون"></رئسئدارة>
			<قسم صنف="هش-3 هشتح-1">
				<شرط>
					<عند تحقق="هدا.مستخدمين.طول">
						<حيز صنف="خطسميك">المستخدمون:</حيز>
					</عند>
				</شرط>
			</قسم>
			<قسم صنف="خلف-رمادي-200 يكبر فائض-تلقا فائض-لمسي هش-3">
				<شرط>
					<عند تحقق="هدا.مستخدمين.طول">
						<شبكي فجوة="12 12" عموصغ="2" عموسط="2">
							<تكرار لكل="مست" في="هدا.مستخدمين">
								<قسم صنف="تئشير:ئطار-ئسود ئطار-2 ئطار-ئبيض خلف-ئبيض مقوس مخطط-طولي فجوة-2 ئرصف-وسط حش-2 ئصفف-وسط">
									<ئيقو صنف="نصكج3" ئسم="دائرة-ملخاص"></ئيقو>
									<حيز صنف="">%{مست.ئسمكامل}</حيز>
									<حيز صنف="نصصج خلف-رمادي-300 حش-1 مقوس">%{مست.صلاحيات.عووض\('ئ'، 'ؤ'\)}</حيز>
								</قسم>
							</تكرار>
						</شبكي>
					</عند>
					<عند غيره>
						<قسم صنف="مخطط ط-ئلاربع ع-كامل ئصفف-وسط ئرصف-وسط">
							<حيز>اضغط \(+\) لإضافة مستخدمين</حيز>
						</قسم>
					</عند>
				</شرط>
			</قسم>
		</قسم>
	)؛
تم

ئعلن مدخل ():
	عنصرمخصص.سججل('مستخدمين'، مستخدمين)؛
تم